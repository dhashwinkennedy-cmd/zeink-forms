{
  "headers": [
    {
      "source": "/(.*).(ts|tsx)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.(?:tsx|ts|js|css|json|png|jpg|jpeg|svg|webp|ico)$).*)",
      "destination": "/index.html"
    }
  ]
}